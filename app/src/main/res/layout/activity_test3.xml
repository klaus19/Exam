"></RadioButton>
                    <RadioButton
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/radioButton36Test3"
                        android:text="D)Rs. 2268
"></RadioButton>
                </RadioGroup>
